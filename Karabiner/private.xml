<?xml version="1.0"?>
<root>

  <vkopenurldef>
    <name>KeyCode::VK_OPEN_URL_iTerm</name>
    <url>file:///Applications/iTerm.app</url>
  </vkopenurldef>
  <vkopenurldef>
    <name>KeyCode::VK_OPEN_URL_Google_Chrome</name>
    <url>file:///Applications/Google%20Chrome.app</url>
  </vkopenurldef>
  <vkopenurldef>
    <name>KeyCode::VK_OPEN_URL_Finder</name>
    <url>file:///System/Library/CoreServices/Finder.app</url>
  </vkopenurldef>
  <vkopenurldef>
    <name>KeyCode::VK_OPEN_URL_Slack</name>
    <url>file:///Applications/Slack.app</url>
  </vkopenurldef>

  <item>
    <name>Change Control+Option+Command+i to open iTerm</name>
    <identifier>private.control_option_command_i</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::I, VK_CONTROL, VK_OPTION, VK_COMMAND,
      KeyCode::VK_OPEN_URL_iTerm,
    </autogen>
  </item>
  <item>
    <name>Change Control+Option+Command+c to open chrome</name>
    <identifier>private.control_option_command_c</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::C, VK_CONTROL, VK_OPTION, VK_COMMAND,
      KeyCode::VK_OPEN_URL_Google_Chrome,
    </autogen>
  </item>
  <item>
    <name>Change Control+Option+Command+f to open finder</name>
    <identifier>private.control_option_command_f</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::F, VK_CONTROL, VK_OPTION, VK_COMMAND,
      KeyCode::VK_OPEN_URL_Finder,
    </autogen>
  </item>
  <item>
    <name>Change Control+Option+Command+s to open finder</name>
    <identifier>private.control_option_command_s</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::S, VK_CONTROL, VK_OPTION, VK_COMMAND,
      KeyCode::VK_OPEN_URL_Slack,
    </autogen>
  </item>

  <item>
    <name>Change Control+W to delete last word</name>
    <identifier>remap.control_w_to_option_delete_last_word</identifier>
    <not>TERMINAL</not>
    <autogen>
      __KeyToKey__
      KeyCode::W, VK_CONTROL,
      KeyCode::DELETE, ModifierFlag::OPTION_L
    </autogen>
  </item>
</root>
